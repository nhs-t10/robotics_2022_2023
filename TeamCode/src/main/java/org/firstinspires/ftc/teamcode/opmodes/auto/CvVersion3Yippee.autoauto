#init:
goto start;

#start:

    closeHand(), next;
    let distance = 3, let rep_times = 0, next;
    webcam.startStreaming(1280, 720, OpenCvCameraRotation.UPRIGHT), after 2s next; // Standardize window size
    goto getColor;

#getColor:
    let color = getAnalysis(), next;
    log(color), next;
    log(getAnalysisTest()), next;
    stopWebcam(), next;
    goto preLoadedCone;

#preLoadedCone:
    ThreadedMoveToPosition(4), next;
    driveOmni(0,-0.5,-0.02), after 48cm next;
    driveOmni(0.5,0,0), after 64cm next;
    driveOmni(0,-0.5,-0.02), after 26cm next;
    driveOmni(0.5,0,0), after 5.5cm next;
    stopDrive(),  next;
    stopThreadedMovement(), after 1s next;
    ThreadedMoveToPosition(2), next;
    goto dropCone;

#dropCone:
    let doneLifting = finishedMoving(), after 0.5s next;
    if(doneLifting){
        log("opening hand"), openHand(), after 2s next;
        goto colorStuffs;
    } else {
        goto dropCone;
    }


#colorStuffs:
    //ThreadedMoveToPosition(3), after 1s next;
    driveOmni(-0.5,0,0), after 0.5cm next;
    stopDrive(), after 0.5s next;
    closeHand(), next;
    ThreadedMoveToPosition(0), next;
    if (color == 1) {
        driveOmni(0,0.5,0.02), after 150cm goto end;
    }
    else if (color == 2) {
        driveOmni(0,0.5,0.02), after 80cm goto end;
    }
    else {
        driveOmni(0,0.5,0.02), after 20cm goto end;
    }

#end:
    log("I'm done"), next;
    stopDrive();
