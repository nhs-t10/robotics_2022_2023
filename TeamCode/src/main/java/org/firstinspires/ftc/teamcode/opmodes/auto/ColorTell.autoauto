goto start;

#start:
    webcam.startStreaming(640, 480, OpenCvCameraRotation.UPRIGHT), next;
    let cameraMonitorViewId = hardwareMap.appContext.getResources().getIdentifier("cameraMonitorViewId", "id", hardwareMap.appContext.getPackageName()), next;
    goto process;

#process:
    log(getColor()), next;
    after 1s goto process;




#end:
    stopWebcam();
    log("i got to the end!");

